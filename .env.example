# Lighter API Configuration
LIGHTER_BASE_URL=https://mainnet.zklighter.elliot.ai

# Your wallet private key (used for setup and account lookup)
LIGHTER_PRIVATE_KEY=0x_your_wallet_private_key_here

# API Key credentials (generated via scripts/setup_api_key.py)
LIGHTER_API_KEY_PRIVATE=your_api_key_private_key_here
LIGHTER_API_KEY_INDEX=3
LIGHTER_ACCOUNT_INDEX=

# Proxy Configuration (required for geo-restricted locations)
# Format: host:port@username:password
PROXY_HOST=res.proxy-seller.com
PROXY_PORT=10000
PROXY_USERNAME=your_proxy_username
PROXY_PASSWORD=your_proxy_password

# Bot Configuration (optional overrides)
POLL_INTERVAL_SECONDS=2
LOG_LEVEL=INFO
